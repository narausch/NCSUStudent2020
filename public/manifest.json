{
  "name": "Flow Diff Viewer",
  "version": "0.1.0",
  "description": "Visualizes the difference between two flow graphs on GitHub.",
  "permissions": ["storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://github.ncsu.edu/*/compare/*"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}